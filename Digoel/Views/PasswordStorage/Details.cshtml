@model Digoel.Models.PasswordStorage

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="container">

    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-info">
                <div class="panel-heading">Details Password Storages</div>
                <div class="panel-body">
                    <div>

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.Website)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Website)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Password)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Password)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.DateSaved)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.DateSaved)
                            </dd>

                        </dl>
                    </div>


                    <p>
                        @Html.ActionLink(" Edit", "Edit", new { id = Model.Id }, new { @class = "btn btn-primary btn-sm glyphicon glyphicon-pencil" }) 
                        @Html.ActionLink(" Back to List", "Index", null, new { @class = "btn btn-info btn-sm glyphicon glyphicon-th-list" })
                    </p>

                </div>
            </div>
        </div>
    </div>


</div>


